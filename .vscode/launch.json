{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "env": {
                "RABBITMQ_HOST": "localhost",
                "RABBITMQ_PORT": "5672",
                "RABBITMQ_USER": "guest",
                "RABBITMQ_PASSWORD": "guest",
                "RABBITMQ_QUEUE": "microkernel-queue"
            }
        }
    ]
}